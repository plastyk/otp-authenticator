{
    "name": "xddesigners/otp-authenticator",
    "description": "A OTP authenticator for use with silverstripe/mfa",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "mfa",
        "2fa",
        "sms",
        "smstools",
        "twilio",
        "authy",
        "security",
        "authentication"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "silverstripe/mfa": "^4.0",
        "twilio/sdk": "^6.35",
        "spomky-labs/otphp": "^10",
        "giggsey/libphonenumber-for-php": "^8.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "XD\\OTPAuthenticator\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
